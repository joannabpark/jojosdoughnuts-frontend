error) {\n            setUsername({\n            error: data.error\n          })\n          } else {\n            localStorage.setItem('app_token', data.token)\n            loginSuccess(data.user)\n            console.log(data.user)\n            toaster.notify(`welcome ${data.user.username}!`, {\n              duration: 2000\n            })\n            history.push('/shop')\n        }\n      })\n    }\n\n    const handleLogout = () => {\n      setOpen(false)\n      logoutSuccess()\n      localStorage.removeItem('app_token')\n    }\n\n    if (location.pathname === \"/\") {\n        return false\n    } else {\n\n    return(\n        <Menu>\n        <Menu.Item\n          name='home'\n          active={activeItem === 'home'}\n          onClick={handleItemClick}\n          as={Link}\n           to={'/home'}\n        >\n           <i aria-hidden=\"true\" className=\"home icon\"></i>Home\n        </Menu.Item>\n        <Menu.Item\n          name='shop'\n          active={activeItem === 'shop'}\n          onClick={handleItemClick}\n          as={Link}\n          to={'/shop'}\n        >\n          <img src=\"https://img.icons8.com/ios/50/000000/doughnut.png\" style={{paddingRight:\"4px\", width:\"23px\"}}/> Shop\n        </Menu.Item>\n        {user.id ? \n        <Menu.Item\n          name='View My Orders'\n          active={activeItem === 'View My Orders'}\n          onClick={handleItemClick}\n          as={Link}\n          to={'/order'}\n        >\n          View My Orders\n        </Menu.Item>\n        : null}\n        <Menu.Item\n          position=\"right\"\n          name='cart'\n          active={activeItem === 'cart'}\n          onClick={handleItemClick}\n          as={Link}\n          to={'/cart'}\n        >\n          <i className=\"shopping cart icon\"></i>Cart\n        </Menu.Item>\n        {!user.id ?\n        <Modal\n        onClose={() => setOpen(false)}\n        onOpen={() => setOpen(true)}\n        open={open}\n        trigger={<Menu.Item\n          name='login'\n          action={activeItem === 'login'}\n          >\n          Login\n          </Menu.Item>\n        }>\n            <Modal.Header as='h2' style={{color:\"turquoise\"}}>\n               Log-in to your account\n             </Modal.Header>\n             <Modal.Content>\n            <Form size='large' onSubmit={handleSubmit}>\n              <Segment stacked>\n                  <Form.Input fluid\n                      icon='user'\n                      iconPosition='left'\n                       type=\"text\" \n                      placeholder=\"username\" \n                       onChange={(e) => setUsername(e.target.value)}\n                       value={username} \n                  />\n                  <Form.Input fluid\n                       icon='lock'\n                       iconPosition='left'\n                      type=\"password\" \n                       onChange={(e) => setPassword(e.target.value)} \n                       value={password} \n                       placeholder=\"password\" \n                 />\n                      <Button style={{backgroundColor:\"pink\"}} fluid size='large' animated='fade' disabled={!validateForm()}>\n                         <Button.Content visible style={{ color: 'turquoise'}}><i aria-hidden=\"true\" className=\"sign in icon\"></i></Button.Content>\n                         <Button.Content hidden style={{ color: 'turquoise'}}>login</Button.Content>\n                     </Button>         \n                   </Segment>\n                 </Form>\n                 <Message> New to us? <a href='/signup'>Sign Up</a>\n                </Message>\n            </Modal.Content>\n          </Modal>\n          :\n          <Menu.Item\n          onClick={handleLogout}\n          name='logout'\n          action={activeItem === 'logout'}\n          as={Link}\n          to={'/shop'}\n          >\n          Logout\n          </Menu.Item>\n         }\n      </Menu>\n    )\n  }\n}\n\nconst mapStateToProps = (state) => {\n  return {\n    user: state.user\n  }\n}\n\nconst mapDispatchToProps = {\n  logoutSuccess,\n  loginSuccess\n}\n\nexport default compose(\n  withRouter,\n  connect(mapStateToProps, mapDispatchToProps)\n)(NavBar);","/Users/jpark/Development/code/commerce-app/commerce-frontend/src/reducers/index.js",[],"/Users/jpark/Development/code/commerce-app/commerce-frontend/src/containers/App.js",["99"],"/Users/jpark/Development/code/commerce-app/commerce-frontend/src/components/Intro.js",["100"],"import React from 'react'\nimport { Button } from 'semantic-ui-react'\nimport PastelDoughnut from '../images/doughnut1.jpeg'\nimport { Link } from 'react-router-dom'\n\nconst Intro = () => {\n    return(\n        <div style = {{backgroundImage: `url(${PastelDoughnut})`, backgroundSize: '100% 100%', height: \"945px\"}}>\n            <Button as={Link} to='/home' animated='fade' style={{position: 'fixed', backgroundColor: 'white', color: 'PaleTurquoise', width: '80px', marginTop: \"200px\"}}>\n                <Button.Content visible style={{textAlign: 'center',}}>Enter</Button.Content>\n                <Button.Content hidden style={{color: 'floralwhite', marginTop:\"-17px\"}}><img src=\"https://img.icons8.com/cotton/64/000000/doughnut.png\" style={{width:\"35px\", height:\"35px\"}}/></Button.Content>\n             </Button>\n        </div>\n    )\n}\n\nexport default Intro","/Users/jpark/Development/code/commerce-app/commerce-frontend/src/components/Home.js",["101"],"/Users/jpark/Development/code/commerce-app/commerce-frontend/src/components/Shop.js",["102","103"],"/Users/jpark/Development/code/commerce-app/commerce-frontend/src/actions/doughnuts.js",[],"/Users/jpark/Development/code/commerce-app/commerce-frontend/src/reducers/doughnuts.js",[],"/Users/jpark/Development/code/commerce-app/commerce-frontend/src/components/Doughnuts.js",[],"/Users/jpark/Development/code/commerce-app/commerce-frontend/src/actions/order.js",[],"/Users/jpark/Development/code/commerce-app/commerce-frontend/src/redu